    "@reach/combobox": "^0.18.0",
    "apollo-boost": "^0.4.9",
    "apollo-server-micro": "^2.17.0",
    "apollo-server-nextjs": "^4.0.0",
    "autoprefixer": "^10.0.2",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.37.3",
    "cloudinary-react": "^1.8.1",
    "cookie": "^0.5.0",
