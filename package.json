{
  "name": "house",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen-old": "apollo client:codegen --target typescript --localSchemaFile schema.gql --outputFlat --includes \"{pages,src}/**\" --excludes \"src/generated/**\" src/generated",
    "codegen:watch": "yarn codegen --watch",
    "db:init": "yarn prisma init",
    "db:generate": "yarn prisma generate",
    "db:migrate:new": "yarn prisma migrate save --experimental",
    "db:migrate:up": "yarn prisma migrate up --experimental",
    "db:migrate:down": "yarn prisma migrate down --experimental"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
